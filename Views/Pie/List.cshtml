@model IEnumerable<SistemasWeb01.Models.Pie>
<!DOCTYPE html>
<html>
   <head>
        <title>Sistemas WEB, Bethesda Pie Shop, hello world!</title>
   </head>
   <body>
       <h1>@ViewBag.CurrentCategory</h1>
    @foreach (var pie in Model)
    {
        <div>
            <h2>@pie.Name</h2>
            <h3>@pie.Price</h3>
            <h4>@pie.Category.CategoryName</h4>
        </div>
    }
</body>
</html>